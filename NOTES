- If a volume is mounted, it might have to be removed manually in order to get the container to mount it properly.



create table lines (
	id serial PRIMARY KEY,
	line_name VARCHAR (50)
);

insert into lines (line_name) values
  ('reds'),
  ('redn'),
  ('golds'),
  ('goldn'),
  ('bluee'),
  ('bluew'),
  ('greene'),
  ('greenw')
;

create table observed_cars (
	car_no INT NOT NULL,
	line INT,
	observation_data DATE
);